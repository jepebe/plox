var x = ((2 + 2.5) * 3) / 4; // grouping

fun calculate(x, y) {
    if (x < y and x != 0) {
        print x + " < " + y;
    } else if (x > y or x < 0) {
        print x + " > " + y;
    } else if (x <= y and x >= y) {
        print x + " == " + y;
    }

    return x + y;  # more comments
}

fun compare(state) {
    print true + ' == ' + state;
    return state == false;
}


var a = 5.5;
var b = 2;

calculate(a, b);
