class Circle {
  init(radius) {
    this.radius = radius;
  }

  area {
    return 3.141592653 * this.radius * this.radius;
  }

  diameter {
    return this.radius * 2;
  }
}

var circle = Circle(4);
print circle.area; // Prints roughly "50.2655".
print circle.diameter;

